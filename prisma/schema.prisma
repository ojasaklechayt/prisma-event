// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Event {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  description String?
  image_url String?
  date DateTime
}
